#! /usr/bin/env bash
PATH_TO_SI=/Volumes/MacintoshHD2/shared/Ancils/HadISST_SICE/N96
OUT_PATH=$PATH_TO_SI

# subset first
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_1999_12_2000_12g -y 1999 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_1999_12_2000_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2000_12_2001_12g -y 2000 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2000_12_2001_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2001_12_2002_12g -y 2001 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2001_12_2002_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2002_12_2003_12g -y 2002 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2002_12_2003_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2003_12_2004_12g -y 2003 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2003_12_2004_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2004_12_2005_12g -y 2004 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2004_12_2005_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2005_12_2006_12g -y 2005 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2005_12_2006_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2006_12_2007_12g -y 2006 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2006_12_2007_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2007_12_2008_12g -y 2007 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2007_12_2008_11g
../subset_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_2008_12_2009_12g -y 2008 -m 12 -n 12 -o $PATH_TO_SI/HadISST_SI_N96_2008_12_2009_11g

# concat
../concat_ancil.py -i $PATH_TO_SI/HadISST_SI_N96_1999_12_2000_11g,$PATH_TO_SI/HadISST_SI_N96_2000_12_2001_11g,$PATH_TO_SI/HadISST_SI_N96_2001_12_2002_11g,$PATH_TO_SI/HadISST_SI_N96_2002_12_2003_11g,$PATH_TO_SI/HadISST_SI_N96_2003_12_2004_11g,$PATH_TO_SI/HadISST_SI_N96_2004_12_2005_11g,$PATH_TO_SI/HadISST_SI_N96_2005_12_2006_11g,$PATH_TO_SI/HadISST_SI_N96_2006_12_2007_11g,$PATH_TO_SI/HadISST_SI_N96_2007_12_2008_11g,$PATH_TO_SI/HadISST_SI_N96_2008_12_2009_11g,$PATH_TO_SI/HadISST_SI_N96_2009_12_2010_12g -o $PATH_TO_SI/HadISST_SI_N96_1999_12_2010_12g

# delete / clean up

rm $PATH_TO_SI/HadISST_SI_N96_1999_12_2000_11g
rm $PATH_TO_SI/HadISST_SI_N96_2000_12_2001_11g
rm $PATH_TO_SI/HadISST_SI_N96_2001_12_2002_11g
rm $PATH_TO_SI/HadISST_SI_N96_2002_12_2003_11g
rm $PATH_TO_SI/HadISST_SI_N96_2003_12_2004_11g
rm $PATH_TO_SI/HadISST_SI_N96_2004_12_2005_11g
rm $PATH_TO_SI/HadISST_SI_N96_2005_12_2006_11g
rm $PATH_TO_SI/HadISST_SI_N96_2006_12_2007_11g
rm $PATH_TO_SI/HadISST_SI_N96_2007_12_2008_11g
rm $PATH_TO_SI/HadISST_SI_N96_2008_12_2009_11g
